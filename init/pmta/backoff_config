    reply /DM000001/                                            mode=backoff
    reply /ES000010/                                            mode=backoff
    reply /RL000001/                                            mode=backoff
    reply /RL000002/                                            mode=backoff
    reply /421 - \[Too many sessions opened\]/                  mode=backoff
    reply /421 - \[Reverse DNS failure : Try again later\]/     mode=backoff
    reply /421 - \[Try again later\]/                           mode=backoff
    reply /452 - \[Too many emails sent on this session\]/      mode=backoff
    reply /452 - \[Too many recipients for message\]/           mode=backoff
    reply /550 - \[Not our customer\]/                          mode=backoff
    reply /550 - \[Your message could not be delivered due to too many invalid recipients\]/ mode=backoff
    reply /550 - \[Invalid sender domain\]/                     mode=backoff
    reply /550 - \[Account not available\]/                     mode=backoff
